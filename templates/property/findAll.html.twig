{% extends 'base.html.twig' %}




{% block title %}Hello PropertyController!{% endblock %}

{% block body %}
    <style>
        .example-wrapper {
            margin: 1em auto;
            max-width: 800px;
            width: 95%;
            font: 18px/1.5 sans-serif;
        }

        .example-wrapper code {
            background: #F5F5F5;
            padding: 2px 6px;
        }
    </style>

    <div class="example-wrapper">
        {% for key,property in base_content %}
            ID : {{ property.id }}
            name : {{ property.name }}
            Postal code : {{ property.postalCode }}
            City : {{ property.city }}
            Country : {{ property.country }}
            name : {{ property.name }}

            <br>
        {% endfor %}
    </div>
{% endblock %}
